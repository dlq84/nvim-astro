{
  "neodev": {
    "library": {
      "enabled": true,
      "plugins": true
    }
  },
  "neoconf": {
    "plugins": {
      "lua_ls": {
        "enabled": true
      }
    }
  },
  "lspconfig": {
    "lua_ls": {
      "Lua.format.enable": false
    },
    "html": {
      "html.format.enable": false
    },
    "eslint": {
      "eslint.format.enable": false
    },
    "tsserver": {
      "javascript.format.enable": false,
      "typescript.format.enable": false
    },
    "vtsls": {
      "javascript.format.enable": false,
      "typescript.format.enable": false
    }
  }
}
